{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "preserve",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "paths": {
      "dv01_solar": [
        "./types/dv01_solar-types"
      ]
    },
  },
  "include": [
    "src",
    "types",
    "types/.dv01_solar-types"
  ]
}
